<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CategoryController</servlet-name>
  	<display-name>CategoryController</display-name>
  	<description>This will handle all the requests related with category managment</description>
  	<servlet-class>com.codewithkrishanthi.controller.CategoryController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>com.codewithkrishanthi.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutServlet</servlet-name>
  	<display-name>LogoutServlet</display-name>
  	<description></description>
  	<servlet-class>com.codewithkrishanthi.controller.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RegisterServlet</servlet-name>
  	<display-name>RegisterServlet</display-name>
  	<description></description>
  	<servlet-class>com.codewithkrishanthi.controller.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DriverServlet</servlet-name>
  	<display-name>DriverServlet</display-name>
  	<description></description>
  	<servlet-class>com.codewithkrishanthi.controller.DriverServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>BookingServlet</servlet-name>
  	<display-name>BookingServlet</display-name>
  	<description></description>
  	<servlet-class>com.codewithkrishanthi.controller.BookingServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FeedbackServlet</servlet-name>
  	<display-name>FeedbackServlet</display-name>
  	<description></description>
  	<servlet-class>com.codewithkrishanthi.controller.FeedbackServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DriverController</servlet-name>
  	<display-name>DriverController</display-name>
  	<description></description>
  	<servlet-class>com.codewithkrishanthi.controller.DriverController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CategoryController</servlet-name>
  	<url-pattern>/getCategory</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CategoryController</servlet-name>
  	<url-pattern>/addCategory</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CategoryController</servlet-name>
  	<url-pattern>/updateCategory</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CategoryController</servlet-name>
  	<url-pattern>/deleteCategory</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutServlet</servlet-name>
  	<url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RegisterServlet</servlet-name>
  	<url-pattern>/addCustomer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BookingServlet</servlet-name>
  	<url-pattern>/getBooking</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BookingServlet</servlet-name>
  	<url-pattern>/addBooking</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BookingServlet</servlet-name>
  	<url-pattern>/updateBooking</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BookingServlet</servlet-name>
  	<url-pattern>/deleteBooking</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FeedbackServlet</servlet-name>
  	<url-pattern>/FeedbackServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DriverController</servlet-name>
  	<url-pattern>/getDriver</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DriverController</servlet-name>
  	<url-pattern>/addDriver</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DriverController</servlet-name>
  	<url-pattern>/updateDriver</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DriverController</servlet-name>
  	<url-pattern>/deleteDriver</url-pattern>
  </servlet-mapping>
</web-app>
